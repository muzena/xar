msgid "lzop (<file>.tar.lzo</file>, <file>.tzo</file>)"
msgstr "lzop (<file>.tar.lzo</file>, <file>.tzo</file>)"

#: C/supported-formats.page:46(item/p)
msgid "7zip (<file>.tar.7z</file>)"
msgstr "7zip (<file>.tar.7z</file>)"

