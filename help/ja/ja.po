msgstr "ZIP でアーカイブ化されたコミックブック (<file>.cbz</file>)"

#: C/supported-formats.page:51(item/p)
msgid "ZIP Archive (<file>.zip</file>)"
msgstr "ZIP アーカイブ (<file>.zip</file>)"

